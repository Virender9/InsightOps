{
  "name": "insightops-enterprise",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:enterprise": "vite --port 5173",
    "mock-api": "json-server --watch db.json --port 4000",
    "dev:with-mock": "concurrently \"npm run mock-api\" \"npm run dev\"",
    "build:production": "NODE_ENV=production npm run build",
    "build:docker": "docker build -t insightops-enterprise .",
    "start:docker": "docker-compose up -d",
    "stop:docker": "docker-compose down",
    "logs:docker": "docker-compose logs -f",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "chart.js": "^4.4.2",
    "chartjs-adapter-date-fns": "^3.0.0",
    "json-server": "^0.17.4",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-vue-next": "^0.536.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.2.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "vite": "^5.1.4"
  }
}
