# InsightOps Dashboard Environment Configuration

# API Configuration
VITE_APP_USE_MOCK_DATA=true

# Application Configuration
VITE_APP_NAME=InsightOps Enterprise Console
VITE_APP_VERSION=2.0.0

# API Mode Configuration
# Options: 'sample' (uses built-in sample data) or 'live' (uses real APIs)
VITE_API_MODE=sample

# Helix Console API Configuration
VITE_HELIX_API_URL=https://helix-console-api.example.com
VITE_HELIX_API_KEY=your_helix_api_key_here
VITE_HELIX_WS_URL=wss://helix-console-api.example.com/ws

# Swift Console API Configuration  
VITE_SWIFT_API_URL=https://swift-console-api.example.com
VITE_SWIFT_API_KEY=your_swift_api_key_here

# ML Backend Configuration
VITE_ML_BACKEND_URL=http://localhost:8000
VITE_ML_API_KEY=your_ml_api_key_here
VITE_ML_MODEL_VERSION=v2.1.0

# Azure OpenAI Configuration
VITE_AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
VITE_AZURE_OPENAI_KEY=your_azure_openai_key_here
VUE_APP_AZURE_OPENAI_DEPLOYMENT=gpt-4-turbo

# Alternative OpenAI Configuration
VUE_APP_OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration (for mock server)
VUE_APP_DB_HOST=localhost
VUE_APP_DB_PORT=4000

# Application Configuration
VUE_APP_REFRESH_INTERVAL=30000
VUE_APP_MAX_ALERTS=1000
VUE_APP_MAX_METRICS_HISTORY=5000
VUE_APP_ENABLE_DEBUG_LOGS=false

# Feature Flags
VUE_APP_ENABLE_AI_FEATURES=true
VUE_APP_ENABLE_ML_DETECTION=true
VUE_APP_ENABLE_REAL_TIME_UPDATES=true
VUE_APP_ENABLE_TEMPERATURE_MONITORING=true

# Monitoring Configuration
VUE_APP_SENTRY_DSN=your_sentry_dsn_here
VUE_APP_ANALYTICS_TRACKING_ID=your_analytics_id_here

# Performance Monitoring
VUE_APP_PERFORMANCE_BUDGET_KB=2048
VUE_APP_ENABLE_PERFORMANCE_MONITORING=true
